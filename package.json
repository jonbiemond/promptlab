{
  "name": "promptlab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ignore-path .gitignore",
    "format": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "jest --config jest.config.cjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/identity": "^4.0.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "next": "14.0.2",
    "next-auth": "^4.24.5",
    "openai": "^4.19.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.0.2",
    "jest": "^29.7.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "postcss": "^8",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.2",
    "ts-jest": "^29.1.1"
  }
}
